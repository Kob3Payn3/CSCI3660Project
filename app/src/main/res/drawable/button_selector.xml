<?xml version="1.0" encoding="utf-8"?>

<!-- Makes the color of the disabled and enabled grid buttons different-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Enabled state -->
    <item android:drawable="@drawable/android_mole" android:state_enabled="true"/>
    <!-- Disabled state -->
    <item android:drawable="@drawable/roundedbutton_disabled" android:state_enabled="false"/>
    <!-- Default State -->
    <item android:drawable="@drawable/roundedbutton" android:state_enabled="true"/>

</selector>